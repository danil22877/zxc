n= int(input())
t = float(input())
total_beta_time = n * t * 7
nick_time = 5
ratio = total_beta_time / nick_time
print(ratio)


name = input()
time = input()
date = input()
year = input()
colon_index = time.index(':')
hours = time [:colon_index]
minutes = time[colon_index + 1:]
code = f"{name[0]}.{hours}{minutes}{date}{year}"
print(code)



