""""""""""
class Phone:
    def __init__(self, brand, model):
        self.brand = brand
        self.model = model
    
    def display_info(self):
        print(f"Бренд: {self.brand} Модель: {self.model}")

# Ввод данных от пользователя
brand_input = input()
model_input = input()

# Создание экземпляра класса и вывод информации
phone = Phone(brand_input, model_input)
phone.display_info()

""""""""""""



def F_iterative(n):
    result = 1
    for i in range(2, n + 1):
        result = 2 * result + 1
    return result

print(F_iterative(6))  # Выведет 63



"""
class Profit:
    def __init__(self, income, expenses):
        self.income = income
        self.expenses = expenses

    def calculate_revenue(self):
        return self.income - self.expenses


# Ввод данных
income = float(input())
expenses = float(input())

# Создание экземпляра класса
project = Profit(income, expenses)

# Расчёт и вывод чистой выручки
revenue = project.calculate_revenue()
print(revenue)





