kody = ["веревка", "нож", "арбалет", "спички", "вода"]
nick = ["нож", "веревка", "вода", "спальник", "рация"]
kody = ["веревка", "нож", "арбалет", "спички", "вода"]
nick = ["нож", "веревка", "вода", "спальник", "рация"]
set_kody = set(kody)
set_nick = set(nick)
print(len(set_kody | set_nick))
print(len(set_kody & set_nick))
